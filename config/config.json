      "components": [
        {
          "entrypoint": "./lib/webparts/corporateLeadership/CorporateLeadershipWebPart.js",
          "manifest": "./src/webparts/corporateLeadership/CorporateLeadershipWebPart.manifest.json"
        }
      ]
    },
    "all-announcements-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/allAnnouncements/AllAnnouncementsWebPart.js",
          "manifest": "./src/webparts/allAnnouncements/AllAnnouncementsWebPart.manifest.json"
        }
      ]
    },
    "announcement-details-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/announcementDetails/AnnouncementDetailsWebPart.js",
          "manifest": "./src/webparts/announcementDetails/AnnouncementDetailsWebPart.manifest.json"
