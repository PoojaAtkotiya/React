    "news-detail-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/newsDetail/NewsDetailWebPart.js",
          "manifest": "./src/webparts/newsDetail/NewsDetailWebPart.manifest.json"
        }
      ]
    },
    "leadership-team-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/leadershipTeam/LeadershipTeamWebPart.js",
          "manifest": "./src/webparts/leadershipTeam/LeadershipTeamWebPart.manifest.json"
        }
      ]
    },
    "leadership-team-detail-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/leadershipTeamDetail/LeadershipTeamDetailWebPart.js",
          "manifest": "./src/webparts/leadershipTeamDetail/LeadershipTeamDetailWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "jquery": {
      "path": "https://esmsyspvt.sharepoint.com/sites/Almatica/SiteAssets/JS/jquery.min.js",
      "globalName": "jquery"
    },
    "jquerySidebar": "https://esmsyspvt.sharepoint.com/sites/Almatica/SiteAssets/JS/jquery.sidebar.js",
    "navigation": "https://esmsyspvt.sharepoint.com/sites/Almatica/SiteAssets/JS/navigation.js",
    "owlcarousel": "https://esmsyspvt.sharepoint.com/sites/Almatica/SiteAssets/JS/owl.carousel.min.js",
    "custom": "https://esmsyspvt.sharepoint.com/sites/Almatica/SiteAssets/JS/custom.js"
  },
  "localizedResources": {
    "DemoWebPartStrings": "lib/webparts/demo/loc/{locale}.js",
    "AnnouncementsWebPartStrings": "lib/webparts/announcements/loc/{locale}.js",
    "TopPerformanceWebPartStrings": "lib/webparts/topPerformance/loc/{locale}.js",
    "EmployeeContactsWebPartStrings": "lib/webparts/employeeContacts/loc/{locale}.js",
    "HeaderFooterApplicationCustomizerStrings": "lib/extensions/headerFooter/loc/{locale}.js",
    "HeadquartersWebPartStrings": "lib/webparts/headquarters/loc/{locale}.js",
    "CorporateLeadershipWebPartStrings": "lib/webparts/corporateLeadership/loc/{locale}.js",
    "AllAnnouncementsWebPartStrings": "lib/webparts/allAnnouncements/loc/{locale}.js",
    "AnnouncementDetailsWebPartStrings": "lib/webparts/announcementDetails/loc/{locale}.js",
    "NewsWebPartStrings": "lib/webparts/news/loc/{locale}.js",
    "AllNewsWebPartStrings": "lib/webparts/allNews/loc/{locale}.js",
    "NewsDetailWebPartStrings": "lib/webparts/newsDetail/loc/{locale}.js",
    "LeadershipTeamWebPartStrings": "lib/webparts/leadershipTeam/loc/{locale}.js",
    "LeadershipTeamDetailWebPartStrings": "lib/webparts/leadershipTeamDetail/loc/{locale}.js"
  }
}